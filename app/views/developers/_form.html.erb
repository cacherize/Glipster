<div class='center'>
	<%= form_for @developer, html: {class: 'form'} do |f| %>
		<h2 class="form_header"><%= params[:action].titleize %> Developer</h2>
		<div class="form_field">
			<%= f.label :name %><br/>
			<%= f.text_field :name %>
		</div>
		<div class="form_field">
			<%= f.label :link %><br/>
			<%= f.text_field :link %>
		</div>
		<div class="form_submit">
			<%= f.submit %>
		</div>
	<% end %>
</div>